  "productName": "Quick Video Editor",
  "version": "0.1.0",
  "description": "Quick Video Editor",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
